{
  "name": "rijksmuseum-schilders",
  "version": "1.0.0",
  "description": "Kijk welke schilders schilderijen hebben in het rijksmuseum",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run build:css && npm run build:img && npm run build:remaining",
    "start": "nodemon server.js",
    "prebuild:css": "rimraf ./static/css",
    "build:css": "node scripts/build-css.js",
    "build:img": "node scripts/build-img.js",
    "build:remaining": "node scripts/build-remaining.js",
    "watch:css": "chokidar 'src/css/*.css' 'src/css/components/*.css' --command 'npm run build:css'",
    "watch:remaining": "chokidar 'src/manifest.json' --command 'npm run build:remaining'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "chokidar-cli": "^2.1.0",
    "dotenv": "^8.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2"
  }
}
